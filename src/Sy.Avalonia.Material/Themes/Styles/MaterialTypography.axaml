<Styles xmlns="https://github.com/avaloniaui"
				 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				 xmlns:c="using:Sy.Avalonia.Material.Controls">
	<Design.PreviewWith>
		<Border Background="White" Padding="32">
			<StackPanel Spacing="32" VerticalAlignment="Top">
				<!-- Display -->
				<Border
						BorderBrush="{DynamicResource md.ref.palette.neutral-variant50}"
						BorderThickness="1"
						CornerRadius="16"
						HorizontalAlignment="Left"
						Padding="24">
					<StackPanel Orientation="Vertical" Spacing="24">
						<TextBlock Classes="display-large" Text="Display Large" />
						<TextBlock Classes="display-medium" Text="Display Medium" />
						<TextBlock Classes="display-small" Text="Display Small" />
					</StackPanel>
				</Border>

				<!-- Headline -->
				<Border
						BorderBrush="{DynamicResource md.ref.palette.neutral-variant50}"
						BorderThickness="1"
						CornerRadius="16"
						HorizontalAlignment="Left"
						Padding="24">
					<StackPanel Orientation="Vertical" Spacing="24">
						<TextBlock Classes="headline-large" Text="Headline Large" />
						<TextBlock Classes="headline-medium" Text="Headline Medium" />
						<TextBlock Classes="headline-small" Text="Headline Small" />
					</StackPanel>
				</Border>

				<!-- Title -->
				<Border
						BorderBrush="{DynamicResource md.ref.palette.neutral-variant50}"
						BorderThickness="1"
						CornerRadius="16"
						HorizontalAlignment="Left"
						Padding="24">
					<StackPanel Orientation="Vertical" Spacing="24">
						<TextBlock Classes="title-large" Text="Title Large" />
						<TextBlock Classes="title-medium" Text="Title Medium" />
						<TextBlock Classes="title-small" Text="Title Small" />
					</StackPanel>
				</Border>

				<!-- Label -->
				<Border
						BorderBrush="{DynamicResource md.ref.palette.neutral-variant50}"
						BorderThickness="1"
						CornerRadius="16"
						HorizontalAlignment="Left"
						Padding="24">
					<StackPanel Orientation="Vertical" Spacing="24">
						<TextBlock Classes="label-large" Text="Label Large" />
						<TextBlock Classes="label-medium" Text="Label Medium" />
						<TextBlock Classes="label-small" Text="Label Small" />
					</StackPanel>
				</Border>

				<!-- Body -->
				<Border
						BorderBrush="{DynamicResource md.ref.palette.neutral-variant50}"
						BorderThickness="1"
						CornerRadius="16"
						HorizontalAlignment="Left"
						Padding="24">
					<StackPanel Orientation="Vertical" Spacing="24">
						<TextBlock Classes="body-large" Text="Body Large" />
						<TextBlock Classes="body-medium" Text="Body Medium" />
						<TextBlock Classes="body-small" Text="Body Small" />
					</StackPanel>
				</Border>

			</StackPanel>
		</Border>
	</Design.PreviewWith>

