		<Setter Property="TextElement.FontSize" Value="{DynamicResource md.sys.typescale.display-large.size}" />
		<Setter Property="TextElement.FontWeight" Value="{DynamicResource md.sys.typescale.display-large.weight}" />
		<Setter Property="TextBlock.LetterSpacing" Value="{DynamicResource md.sys.typescale.display-large.tracking}" />
