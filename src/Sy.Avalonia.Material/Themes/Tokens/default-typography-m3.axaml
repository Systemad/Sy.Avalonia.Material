	<!-- Label -->
	<mux:Alias   x:Key="md.sys.typescale.label-large.font" ResourceKey="md.ref.typeface.plain" />
	<sys:Double   x:Key="md.sys.typescale.label-large.line-height">20</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.label-large.offset">2</sys:Double>
	<sys:Double   x:Key="md.sys.typescale.label-large.size">14</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.label-large.tracking">0.1</sys:Double>
	<mux:Alias   x:Key="md.sys.typescale.label-large.weight" ResourceKey="md.ref.typeface.weight-medium" />
	<mux:Alias   x:Key="md.sys.typescale.label-large.weight.prominent" ResourceKey="md.ref.typeface.weight-bold" />

	<mux:Alias   x:Key="md.sys.typescale.label-medium.font" ResourceKey="md.ref.typeface.plain" />
	<sys:Double   x:Key="md.sys.typescale.label-medium.line-height">16</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.label-medium.offset">2</sys:Double>
	<sys:Double   x:Key="md.sys.typescale.label-medium.size">12</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.label-medium.tracking">0.5</sys:Double>
	<mux:Alias   x:Key="md.sys.typescale.label-medium.weight" ResourceKey="md.ref.typeface.weight-medium" />
	<mux:Alias   x:Key="md.sys.typescale.label-medium.weight.prominent" ResourceKey="md.ref.typeface.weight-bold" />

	<mux:Alias   x:Key="md.sys.typescale.label-small.font" ResourceKey="md.ref.typeface.plain" />
	<sys:Double   x:Key="md.sys.typescale.label-small.line-height">16</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.label-small.offset">2</sys:Double>
	<sys:Double   x:Key="md.sys.typescale.label-small.size">11</sys:Double>
