<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
										xmlns:mux="using:Sy.Avalonia.Material.Markup.Extensions"
										xmlns:sys="using:System">
	
	<!-- NOTE: The md.ref.typescale.*.offset tokens are not from the Material Design spec -->
	<!--       Text was clipped on the bottom, so offset values were found to realign them as  -->
	<!--       close to the original as possible -->
	
	<!-- Reference -->
	<FontFamily  x:Key="md.ref.typeface.plain">avares://Sy.Avalonia.Material/Fonts/Roboto/Roboto#Roboto,$Default</FontFamily>
	<FontWeight  x:Key="md.ref.typeface.weight-regular">400</FontWeight>
	<FontWeight  x:Key="md.ref.typeface.weight-medium">500</FontWeight>
	<FontWeight  x:Key="md.ref.typeface.weight-bold">700</FontWeight>

	<!-- Display -->
	<mux:Alias   x:Key="md.sys.typescale.display-large.font" ResourceKey="md.ref.typeface.plain" />
	<sys:Double   x:Key="md.sys.typescale.display-large.line-height">64</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.display-large.offset">12</sys:Double>
	<sys:Double   x:Key="md.sys.typescale.display-large.size">57</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.display-large.tracking">-0.25</sys:Double>
	<mux:Alias   x:Key="md.sys.typescale.display-large.weight" ResourceKey="md.ref.typeface.weight-regular" />

	<mux:Alias   x:Key="md.sys.typescale.display-medium.font" ResourceKey="md.ref.typeface.plain" />
	<sys:Double   x:Key="md.sys.typescale.display-medium.line-height">52</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.display-medium.offset">10</sys:Double>
	<sys:Double   x:Key="md.sys.typescale.display-medium.size">45</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.display-medium.tracking">0</sys:Double>
	<mux:Alias   x:Key="md.sys.typescale.display-medium.weight" ResourceKey="md.ref.typeface.weight-regular" />

	<mux:Alias   x:Key="md.sys.typescale.display-small.font" ResourceKey="md.ref.typeface.plain" />
	<sys:Double   x:Key="md.sys.typescale.display-small.line-height">44</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.display-small.offset">11</sys:Double>
	<sys:Double   x:Key="md.sys.typescale.display-small.size">36</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.display-small.tracking">0</sys:Double>
	<mux:Alias   x:Key="md.sys.typescale.display-small.weight" ResourceKey="md.ref.typeface.weight-regular" />

	<!-- Headline -->
	<mux:Alias   x:Key="md.sys.typescale.headline-large.font" ResourceKey="md.ref.typeface.plain" />
	<sys:Double   x:Key="md.sys.typescale.headline-large.line-height">40</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.headline-large.offset">5</sys:Double>
	<sys:Double   x:Key="md.sys.typescale.headline-large.size">32</sys:Double>
