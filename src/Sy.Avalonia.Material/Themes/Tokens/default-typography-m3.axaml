	<mux:Alias   x:Key="md.sys.typescale.headline-small.weight" ResourceKey="md.ref.typeface.weight-regular" />

	<!-- Title -->
	<mux:Alias   x:Key="md.sys.typescale.title-large.font" ResourceKey="md.ref.typeface.plain" />
