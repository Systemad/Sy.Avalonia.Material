	<sys:Double  x:Key="md.sys.typescale.title-small.offset">2</sys:Double>
	<sys:Double   x:Key="md.sys.typescale.title-small.size">14</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.title-small.tracking">0.1</sys:Double>
	<mux:Alias   x:Key="md.sys.typescale.title-small.weight" ResourceKey="md.ref.typeface.weight-medium" />

	<!-- Body -->
	<mux:Alias   x:Key="md.sys.typescale.body-large.font" ResourceKey="md.ref.typeface.plain" />
	<sys:Double   x:Key="md.sys.typescale.body-large.line-height">24</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.body-large.offset">2</sys:Double>
	<sys:Double   x:Key="md.sys.typescale.body-large.size">16</sys:Double>
	<sys:Double  x:Key="md.sys.typescale.body-large.tracking">0.5</sys:Double>
	<mux:Alias   x:Key="md.sys.typescale.body-large.weight" ResourceKey="md.ref.typeface.weight-regular" />

	<mux:Alias   x:Key="md.sys.typescale.body-medium.font" ResourceKey="md.ref.typeface.plain" />
