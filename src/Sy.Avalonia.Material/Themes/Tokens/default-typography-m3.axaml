	<mux:Alias   x:Key="md.sys.typescale.title-large.weight" ResourceKey="md.ref.typeface.weight-regular" />

	<mux:Alias   x:Key="md.sys.typescale.title-medium.font" ResourceKey="md.ref.typeface.plain" />
