	<mux:Alias   x:Key="md.sys.typescale.headline-medium.weight" ResourceKey="md.ref.typeface.weight-regular" />

	<mux:Alias   x:Key="md.sys.typescale.headline-small.font" ResourceKey="md.ref.typeface.plain" />
	<sys:Double   x:Key="md.sys.typescale.headline-small.line-height">32</sys:Double>
